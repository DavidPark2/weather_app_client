<div class='container'>
	<div class='row'>
		<h1 class='text-center'>{{location}}</h1>
	</div>
	<div class='row'>
		<div class='col-md-6'>
			<h1 class='text-center'>{{temp}}&deg;</h1>
		</div>
		<div class='col-md-6'>
			<h4 class='text-center'>humidity: {{humidity}}%</h4>
		</div>
	</div>
	<div class='row'>
		<div class='col-md-6'>
			<h4 class='text-center'>Feels like: {{feelLike}}&deg;</h4>
		</div>
		<div class='col-md-6'>
			<h4 class='text-center'>Wind Speed: {{wind}} mph</h4>
		</div>
	</div>
		<div class='row'>
		<div class='col-md-6'>
			<h4 class='text-center'>{{summary}}</h4>
		</div>
		<div class='col-md-6'>
			<h4 class='text-center'>Visibility: {{visibility}} miles</h4>
		</div>
	</div>
		<div class='row'>
		<div class='col-md-6'>
			<h4 class='text-center'>Sunrise: {{sunrise}}</h4>
		</div>
		<div class='col-md-6'>
			<h4 class='text-center'>Dewpoint: {{dewpoint}}</h4>
		</div>
	</div>
		<div class='row'>
		<div class='col-md-6'>
			<h4 class='text-center'>Sunset: {{sunset}}</h4>
		</div>
		<div class='col-md-6'>
			<h4 class='text-center'>Chance of Rain: {{rain}}%</h4>
		</div>
	</div>
</div>